#: gui.py:2269
msgid "User commands:\n"
msgstr "Εντολές χρηστών:\n"

#: gui.py:2270
msgid "r N         - run the simulation for N cycles\n"
msgstr "r N - τρέξτε τη προσομοίωση για N κύκλους \n"

#: gui.py:2271
msgid "c N         - continue the simulation for N cycles\n"
msgstr "r N - τρέχει την προσομοίωση για N κύκλους\n"

#: gui.py:2272
msgid "s X N     - set switch X to N (0 or 1)\n"
msgstr "s X N     - set switch X to N (0 or 1)\n"

#: gui.py:2273
msgid "m X       - set a monitor on signal X\n"
msgstr "m X - ορίστε μια οθόνη στο σήμα X\n"

#: gui.py:2274
msgid "z X         - zap the monitor on signal X\n"
msgstr "z X - ενεργοποιήστε την οθόνη στο σήμα X\n"

#: gui.py:2275
msgid "h             - help (this command)\n"
msgstr "h - βοήθεια (αυτή η εντολή) \n"

#: gui.py:2276
msgid "q            - quit the program"
msgstr "q - κλείστε το πρόγραμμα"

