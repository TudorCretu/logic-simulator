DEVICES
SwSet = SWITCH/0,
SwClear = SWITCH/0,
SwData = SWITCH/0,
Ck = CLOCK/1,
Dtype = DTYPE;

CONNECTIONS
SwSet = Dtype.SET,
SwClear = Dtype.CLEAR,
SwData = Dtype.DATA,
Ck = Dtype.CLK;

MONITORS
Dtype.Q,
Dtype.QBAR;
